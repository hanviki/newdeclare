<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.mrbird.febs.scm.dao.VMsgVendorauditMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="cc.mrbird.febs.scm.entity.VMsgVendoraudit">
                    <result column="CODE" property="code"/>
                    <result column="NAME" property="name"/>
                    <result column="vxCode" property="vxCode"/>
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
        CODE, NAME, vxCode
    </sql>
    <update id="updateVMsgVendoraudit" parameterType="cc.mrbird.febs.scm.entity.VMsgVendoraudit">
        update v_msg_vendoraudit
        <trim prefix="set" suffixOverrides=",">
<if test="code != null">CODE=#{code},</if>
<if test="name != null">NAME=#{name},</if>
<if test="vxCode != null">vxCode=#{vxCode},</if>
        </trim>
        where id=#{id}
    </update>
</mapper>